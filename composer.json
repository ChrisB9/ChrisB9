{
  "name": "cbenco/website",
  "description": "Personal Website",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Christian Rodriguez Benthake",
      "email": "git@cben.co"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "require": {
    "php": "^8.2",
    "symfony/http-foundation": "^6.2",
    "symfony/asset": "^6.2",
    "symfony/twig-bridge": "^6.2",
    "symfony/flex": "^1.19",
    "symfony/framework-bundle": "^6.2",
    "symfony/http-kernel": "^6.2",
    "symfony/dotenv": "^6.2",
    "doctrine/cache": "^2.2.0",
    "erusev/parsedown": "1.8.0-beta-7",
    "erusev/parsedown-extra": "0.8.0",
    "symfony/webpack-encore-bundle": "^1.16",
    "symfony/web-link": "^6.2",
    "nette/utils": "^3.2",
    "doctrine/annotations": "^2.0.0",
    "knplabs/knp-time-bundle": "^1.20",
    "presta/sitemap-bundle": "^3.3.0",
    "cbenco/timestamp-generator": "^0.1.1",
    "ext-json": "*",
    "cmfcmf/openweathermap-php-api": "dev-php-8.2",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/twig-bundle": "^6.2",
    "http-interop/http-factory-guzzle": "^1.0",
    "nelmio/security-bundle": "^3.0",
    "taufik-nurrohman/parsedown-extra-plugin": "^v1.3.9"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "symfony/stopwatch": "^5.2",
    "symfony/web-profiler-bundle": "^5.2"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
